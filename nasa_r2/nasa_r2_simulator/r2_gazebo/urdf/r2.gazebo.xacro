  <gazebo_link name="${robot_name}left_thumb_base"          gravityOff="false" static="false" collide="false"/>
  <gazebo_link name="${robot_name}left_thumb_medial_prime"  gravityOff="false" static="false" collide="false"/>
  <gazebo_link name="${robot_name}left_thumb_medial"        gravityOff="false" static="false" collide="false"/>
  <gazebo_link name="${robot_name}left_thumb_distal"        gravityOff="false" static="false" collide="false"/>

  <gazebo_link name="${robot_name}left_index_base"      gravityOff="false" static="false" collide="false"/>
  <gazebo_link name="${robot_name}left_index_proximal"  gravityOff="false" static="false" collide="false"/>
