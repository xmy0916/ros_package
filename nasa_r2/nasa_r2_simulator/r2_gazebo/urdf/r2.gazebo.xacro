<?xml version="1.0"?>
<robot>

  <macro name="gazebo_link" params="name gravityOff static collide">
     <gazebo reference="${name}">
        <selfCollide>${collide}</selfCollide>
        <static>${static}</static>
        <turnGravityOff>${gravityOff}</turnGravityOff>
     </gazebo>
  </macro>

  <macro name="gazebo_joint" params="name">
     <gazebo reference="${name}">
        <stopKd value="0.0" />
        <stopKp value="1000.0" />
     </gazebo>
  </macro>

  <gazebo_link name="${robot_name}waist_center" gravityOff="false" static="false" collide="false"/>
  <gazebo_joint name="${robot_name}waist/joint0"/>

  <gazebo_link name="${robot_name}left_shoulder_roll"  gravityOff="false" static="false" collide="false"/>
  <gazebo_link name="${robot_name}left_shoulder_pitch" gravityOff="false" static="false" collide="false"/>
  <gazebo_link name="${robot_name}left_upper_arm"      gravityOff="false" static="false" collide="false"/>
  <gazebo_link name="${robot_name}left_elbow"          gravityOff="false" static="false" collide="false"/>
  <gazebo_link name="${robot_name}left_lower_arm"      gravityOff="false" static="false" collide="false"/>
  <gazebo_link name="${robot_name}left_wrist_pitch"    gravityOff="false" static="false" collide="false"/>
  <gazebo_link name="${robot_name}left_wrist_yaw"      gravityOff="false" static="false" collide="false"/>
  <gazebo_link name="${robot_name}left_palm"           gravityOff="false" static="false" collide="false"/>

  <gazebo_joint name="${robot_name}left_arm/joint0"/>
  <gazebo_joint name="${robot_name}left_arm/joint1"/>
