  <gazebo_link name="${robot_name}right_index_medial"    gravityOff="false" static="false" collide="false"/>
  <gazebo_link name="${robot_name}right_index_distal"    gravityOff="false" static="false" collide="false"/>

  <gazebo_link name="${robot_name}right_middle_base"      gravityOff="false" static="false" collide="false"/>
  <gazebo_link name="${robot_name}right_middle_proximal"  gravityOff="false" static="false" collide="false"/>
  <gazebo_link name="${robot_name}right_middle_medial"    gravityOff="false" static="false" collide="false"/>
