  <gazebo_joint name="${robot_name}neck/joint0"/>
  <gazebo_link name="${robot_name}neck_lower_pitch"  gravityOff="false" static="false" collide="false"/>
  <gazebo_joint name="${robot_name}neck/joint1"/>
  <gazebo_link name="${robot_name}neck_roll"         gravityOff="false" static="false" collide="false"/>
