  <property name="right_arm_j2_damping" value= "10.0" />
  <property name="right_arm_j2_friction" value= "0.0" />
  <property name="right_arm_j3_damping" value= "1.0" />
  <property name="right_arm_j3_friction" value= "0.0" />
