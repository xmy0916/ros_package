  </joint>

  <link name="${robot_name}right_shoulder_roll">
    <visual>
      <geometry>
        <mesh filename="package://r2_description/meshes/Right_Shoulder_Upper.dae"/>
      </geometry>
      <material name="dirty_white"/>
      <origin rpy="0 0 2.25" xyz="0 0 .0254"/>
    </visual>
