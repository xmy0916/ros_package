        <origin xyz="0.0089916 -0.00131572  0.10287"/>
        <mass value="6.168856232"/>
        <inertia ixx="0.0198585269"  ixy="0.0060195977"  ixz="0.0003950635"  iyy="0.0671900643"  iyz="0.00012683"  izz="0.0634150128"/>
    </inertial>
